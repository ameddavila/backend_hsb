    "prepare": "npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "fast-glob": "^3.3.2",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "module-alias": "^2.2.3",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "tedious": "^18.6.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
